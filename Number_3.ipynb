   "source": [
    "\"Scalar and one dimensional array\"\n",
    "\n",
    "# broadcasting scalar to one-dimensional array\n",
    "import numpy as np\n",
    "\n",
    "a = np.array([1, 2, 3])\n",
    "print(f\"a = {a}\")\n",
    "\n",
    "# define scalar\n",
    "b = 2\n",
    "print(f\"b = {b}\")\n",
    "\n",
    "# broadcast\n",
    "c = a + b\n",
    "print(f\"c = {c}\")"
   ],
